{
  "name": "Marabel",
  "version": "0.1.0",
  "description": "3d browser RPG game",
  "main": "dist/server.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "tsc",
    "heroku-postbuild": "tsc",
    "watch:build": "tsc --w",
    "start": "node ./dist/server.js",
    "start-dev": "nodemon dist/server.js",
    "dev": "npm-run-all -p watch:build start-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nimusco/marabel.git"
  },
  "author": "Nicolas Musco",
  "dependencies": {
    "@colyseus/loadtest": "0.14.7",
    "@colyseus/monitor": "0.14.22",
    "@colyseus/ws-transport": "0.14.21",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "ByteBuffer": "^0.3.0",
    "colyseus": "^0.14.17",
    "cors": "^2.8.5",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.4"
  }
}
